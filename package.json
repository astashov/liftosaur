{
  "name": "liftosaur",
  "version": "1.0.0",
  "description": "App for tracking weight lifting progress",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn docs && NODE_ENV=production yarn build:dev",
    "publish": "wrangler publish --env production",
    "analyze": "open dist/report.html",
    "docs": "generate-md --layout ./src/docs/layout/ --input ./src/docs/content/ --output ./docs",
    "build:server": "webpack --config webpack.server.config.js",
    "build:dev": "webpack",
    "lint": "tsc --listFiles | grep -v 'node_modules' | xargs eslint",
    "start": "webpack-dev-server",
    "start:server": "wrangler dev --host local-api.liftosaur.com",
    "test": "jest"
  },
  "author": "Anton Astashov",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/wrangler": "^1.8.4",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/codemirror": "^0.0.96",
    "@types/cookie": "^0.3.3",
    "@types/deepmerge": "^2.2.0",
    "@types/gapi": "^0.0.39",
    "@types/gapi.auth2": "^0.0.51",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@udacity/types-service-worker-mock": "^1.1.2",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "cypress": "^5.1.0",
    "cypress-terminal-report": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.7",
    "markdown-styles": "^3.1.10",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "rollbar": "^2.16.2",
    "style-loader": "^1.1.3",
    "tailwindcss": "^1.2.0",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "types-cloudflare-worker": "^1.1.2",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "codemirror": "^5.54.0",
    "cookie": "^0.4.0",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.2",
    "fp-ts": "^2.6.2",
    "idb-keyval": "^3.2.0",
    "io-ts": "^2.2.4",
    "jsonwebtoken": "^8.5.1",
    "preact": "^10.3.2",
    "uplot": "^1.0.9"
  }
}
